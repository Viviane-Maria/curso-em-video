algoritmo "Teste1"
var
   Nome: caractere
   N1, D: real
inicio
      Escreva ("Olá, por favor, digite seu nome ")
      Leia (Nome)
      Escreval ("Seja bem vindo(a), ", Nome, "!")
      Escreva ("Digite o ano em que nasceu ")
      Leia (N1)
      D <- (2020-N1)
      Escreval ("Sua idade é ", D)
      Escreva ("Obrigada por utilizar o programa")
fimalgoritmo